{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-name"
		},
		"AzureSqlDatabaseLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseLinkedService'"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorageLinkedService'"
		},
		"DataLakeStorageGen2USE_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLakeStorageGen2USE'"
		},
		"WestEurope_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'WestEurope'"
		},
		"DataLakeStorageGen2USE_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://samie1628.dfs.core.windows.net/"
		},
		"FileInUSE_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/5e3290d1-4bb1-480d-8da8-6e1ccf3e878d/resourceGroups/RG_MIE1628/providers/Microsoft.Storage/storageAccounts/samie1628"
		},
		"FileInWestEurope_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/5e3290d1-4bb1-480d-8da8-6e1ccf3e878d/resourceGroups/RG_MIE1628/providers/Microsoft.Storage/storageAccounts/samie1628westeurope"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-name_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabaseLinkedService_connectionString": {
						"value": "[parameters('AzureSqlDatabaseLinkedService_connectionString')]"
					},
					"AzureStorageLinkedService_connectionString": {
						"value": "[parameters('AzureStorageLinkedService_connectionString')]"
					},
					"DataLakeStorageGen2USE_accountKey": {
						"value": "[parameters('DataLakeStorageGen2USE_accountKey')]"
					},
					"WestEurope_connectionString": {
						"value": "[parameters('WestEurope_connectionString')]"
					},
					"DataLakeStorageGen2USE_properties_typeProperties_url": {
						"value": "[parameters('DataLakeStorageGen2USE_properties_typeProperties_url')]"
					},
					"FileInUSE_properties_typeProperties_scope": {
						"value": "[parameters('FileInUSE_properties_typeProperties_scope')]"
					},
					"FileInWestEurope_properties_typeProperties_scope": {
						"value": "[parameters('FileInWestEurope_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}